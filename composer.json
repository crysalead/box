{
    "name": "crysalead/box",
    "description": "Minimalist Dependency Injection Container.",
    "keywords": ["IoC", "Dependency Injection", "Dependency Injection Container", "Inversion of Control"],
    "license": "MIT",
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "kahlan/kahlan": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Lead\\Box\\": "src/"
        },
        "files": ["src/init.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Lead\\Box\\Spec\\": "spec/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}

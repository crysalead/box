{
    "name": "crysalead/box",
    "description": "Minimalist Dependency Injection Container.",
    "keywords": ["IoC", "Dependency Injection", "Dependency Injection Container", "Inversion of Control"],
    "license": "MIT",
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "crysalead/kahlan": "dev-master"
    },
    "autoload": {
        "psr-4": {
			"box\\": "src/",
			"box\\spec\\": "spec/"
		},
        "files": ["src/init.php"]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev"
}
